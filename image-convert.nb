Cell[CellGroupData[{

Cell["\<\
If we want to write an include file which describes the image we' ve just \
made in a C - style set of constants like the Heltec demo code did, we can do \
the following :\
\>", "Subsubsection",
 CellChangeTimes->{{3.7790361395840683`*^9, 
  3.779036191033394*^9}},ExpressionUUID->"5e2b30bb-5dce-4dbd-818b-\
58dfb5f1cd8c"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"blackcode", " ", "=", " ", 
   RowBox[{
   "\"\<const unsigned char IMAGE_BLACK[] PROGMEM = \>\"", " ", "<>", 
    RowBox[{"ToString", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"FromDigits", "[", 
        RowBox[{"#", ",", "2"}], "]"}], " ", "&"}], "/@", 
      RowBox[{"Partition", "[", 
       RowBox[{
        RowBox[{"Flatten", "[", 
         RowBox[{"ImageData", "[", 
          RowBox[{"blackchannel", ",", "\"\<Bit\>\""}], "]"}], "]"}], ",", 
        "8"}], "]"}]}], "]"}], " ", "<>", " ", "\"\<;\>\""}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"redcode", " ", "=", " ", 
   RowBox[{"\"\<const unsigned char IMAGE_RED[] PROGMEM = \>\"", " ", "<>", 
    RowBox[{"ToString", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"FromDigits", "[", 
        RowBox[{"#", ",", "2"}], "]"}], " ", "&"}], "/@", 
      RowBox[{"Partition", "[", 
       RowBox[{
        RowBox[{"Flatten", "[", 
         RowBox[{"ImageData", "[", 
          RowBox[{"redchannel", ",", "\"\<Bit\>\""}], "]"}], "]"}], ",", 
        "8"}], "]"}]}], "]"}], " ", "<>", " ", "\"\<;\>\""}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"outfile", " ", "=", " ", 
   RowBox[{"OpenWrite", "[", "\"\<~/badge/imagedata-new.cpp\>\"", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"WriteString", "[", 
   RowBox[{"outfile", ",", "\"\<#include \\\"imagedata.h\\\">\\n\>\""}], 
   "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"WriteString", "[", 
   RowBox[{"outfile", ",", "\"\<#include <avr/pgmspace.h>\\n\>\""}], "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"WriteString", "[", 
   RowBox[{"outfile", ",", 
    RowBox[{"blackcode", " ", "<>", " ", "\"\<\\n\>\""}]}], "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"WriteString", "[", 
   RowBox[{"outfile", ",", 
    RowBox[{"redcode", " ", "<>", " ", "\"\<\\n\\n\>\""}]}], "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Close", "[", "outfile", "]"}]}], "Input",
 CellChangeTimes->{{3.75847541002048*^9, 3.758475498824027*^9}, {
  3.758552901677376*^9, 3.75855294579016*^9}},
