  3.7790362461787243`*^9}},ExpressionUUID->"f6a78198-8c0c-4c24-9b07-\
25bc2323e1d1"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"outfile", " ", "=", " ", 
   RowBox[{"OpenWrite", "[", 
    RowBox[{"\"\<~/badge/example.img\>\"", ",", 
     RowBox[{"BinaryFormat", "\[Rule]", "True"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"BinaryWrite", "[", 
   RowBox[{"outfile", ",", 
    RowBox[{
     RowBox[{
      RowBox[{"FromDigits", "[", 
       RowBox[{"#", ",", "2"}], "]"}], " ", "&"}], "/@", 
     RowBox[{"Partition", "[", 
      RowBox[{
       RowBox[{"Flatten", "[", 
        RowBox[{"ImageData", "[", 
         RowBox[{"blackchannel", ",", "\"\<Bit\>\""}], "]"}], "]"}], ",", 
       "8"}], "]"}]}]}], "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"BinaryWrite", "[", 
   RowBox[{"outfile", ",", 
    RowBox[{
     RowBox[{
      RowBox[{"FromDigits", "[", 
       RowBox[{"#", ",", "2"}], "]"}], " ", "&"}], "/@", 
     RowBox[{"Partition", "[", 
